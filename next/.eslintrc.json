{
  "env": {
    "browser": true,
    "es2021": true,
    "amd": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next/core-web-vitals",
    "next/babel"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "template-curly-spacing": ["error", "never"],
    "no-else-return": "warn",
    "callback-return": "error",
    "default-case": "error",
    "getter-return": "warn",
    "handle-callback-err": "warn",
    "newline-per-chained-call": "warn",
    "no-console": "warn",
    "no-debugger": "warn",
    "prefer-const": "error",
    "require-await": "error",
    "no-multi-str": "error"
  }
}
